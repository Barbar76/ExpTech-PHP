{% extends "ORMMainBundle::layout.html.twig" %}

{% block title %}{{ parent() }} - Get By Date Action{% endblock %}

{% block body %}
    {% if person is not empty %}
        <div class="text-center">
            <i class="glyphicon glyphicon-time"></i>
            Time elapsed : {{ time }} milliseconds
            <br>
            <i class="glyphicon glyphicon-stats"></i>
            Person count : {{ person }}
        </div>
    {% endif %}

{% endblock %}